<% title _("New OpenStack Cluster") %>

<%= form_for @cluster do |f| %>
  <%= base_errors_for @cluster %>

  <%= f.fields_for :subnet, f.object.subnet do |sf| %>
        <%= render 'subnets/fields', :f => sf %>
    <% end %>

<%= submit_or_cancel f, false, {:cancel_path => hostgroups_path} %>
 <% end %>
